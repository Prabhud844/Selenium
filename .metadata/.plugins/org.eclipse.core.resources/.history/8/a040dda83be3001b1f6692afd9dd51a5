package Test.Test;

import java.util.ArrayList;
import java.util.List;

public class nonrepatestring {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Write a program to find out first non repeated character from input String?
		
		System.out.println(printNoReptingcharcter("abcaabcdedxy"));

	}

	private static Character printNoReptingcharcter(String inpustring) {
		char[] chars =inpustring.toCharArray();
		
		List<Character> discahrdchars = new ArrayList<Character>();
		for (int i =0;i<=chars.length;i++){
			char c =chars[i];
			
			if (discahrdchars.contains(c)) {
				continue;
				for(int j=i+1;j<chars.length;j++){
					if (c==chars[j]){
						{
							discahrdchars.add(c);
							break;
						
					}
						
						
				}
			}
		}
		
		return null;
		
	}
	
}
